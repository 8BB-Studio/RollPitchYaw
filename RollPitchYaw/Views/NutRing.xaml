<UserControl x:Class="RollPitchYaw.Views.NutRing"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:RollPitchYaw.Views"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="170"
             d:DesignWidth="170"
             mc:Ignorable="d">
    <Grid>
        <Canvas>
            <local:Nut Canvas.Left="144" Canvas.Top="75">
                <local:Nut.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.2" ScaleY="0.2" />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </local:Nut.RenderTransform>
            </local:Nut>
            <local:Nut Canvas.Left="123" Canvas.Top="123">
                <local:Nut.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.2" ScaleY="0.2" />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </local:Nut.RenderTransform>
            </local:Nut>
            <local:Nut Canvas.Left="75" Canvas.Top="144">
                <local:Nut.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.2" ScaleY="0.2" />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </local:Nut.RenderTransform>
            </local:Nut>
            <local:Nut Canvas.Left="27" Canvas.Top="123">
                <local:Nut.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.2" ScaleY="0.2" />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </local:Nut.RenderTransform>
            </local:Nut>
            <local:Nut Canvas.Left="6" Canvas.Top="75">
                <local:Nut.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.2" ScaleY="0.2" />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </local:Nut.RenderTransform>
            </local:Nut>
            <local:Nut Canvas.Left="27" Canvas.Top="27">
                <local:Nut.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.2" ScaleY="0.2" />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </local:Nut.RenderTransform>
            </local:Nut>
            <local:Nut Canvas.Left="75" Canvas.Top="6">
                <local:Nut.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.2" ScaleY="0.2" />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </local:Nut.RenderTransform>
            </local:Nut>
            <local:Nut Canvas.Left="123" Canvas.Top="27">
                <local:Nut.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform ScaleX="0.2" ScaleY="0.2" />
                        <SkewTransform />
                        <RotateTransform />
                        <TranslateTransform />
                    </TransformGroup>
                </local:Nut.RenderTransform>
            </local:Nut>

        </Canvas>
    </Grid>
</UserControl>
