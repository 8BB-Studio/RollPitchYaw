<UserControl x:Class="RollPitchYaw.Views.SocketView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:RollPitchYaw.Views"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Width="200"
             Height="200"
             mc:Ignorable="d">
    <Canvas Width="200" Height="200">
        <Ellipse Canvas.Left="90"
                 Canvas.Top="0"
                 Width="20"
                 Height="20"
                 Stroke="OrangeRed"
                 StrokeThickness="3" />
        <Ellipse Canvas.Left="0"
                 Canvas.Top="90"
                 Width="20"
                 Height="20"
                 Stroke="OrangeRed"
                 StrokeThickness="3" />
        <Ellipse Canvas.Left="180"
                 Canvas.Top="90"
                 Width="20"
                 Height="20"
                 Stroke="OrangeRed"
                 StrokeThickness="3" />
        <Ellipse Canvas.Left="90"
                 Canvas.Top="180"
                 Width="20"
                 Height="20"
                 Stroke="OrangeRed"
                 StrokeThickness="3" />
    </Canvas>
</UserControl>
